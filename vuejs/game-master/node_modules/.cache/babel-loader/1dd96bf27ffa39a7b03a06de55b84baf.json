{"ast":null,"code":"export default {\n  props: {\n    height: {\n      type: Number,\n      required: true\n    }\n  },\n\n  data() {\n    return {\n      gameStarted: false\n    };\n  },\n\n  mounted() {},\n\n  methods: {\n    startGame() {\n      this.gameStarted = true;\n      this.addObjects();\n      this.draw();\n      window.addEventListener('keydown', this.moveBasket);\n    },\n\n    addObjects() {\n      const numObjects = 1;\n\n      for (let i = 0; i < numObjects; i++) {\n        const object = {\n          x: Math.floor(Math.random() * this.width),\n          y: this.height - Math.floor(Math.random() * (this.height / 2)),\n          width: 50,\n          height: 50,\n          color: '#' + Math.floor(Math.random() * 16777215).toString(16),\n          speed: Math.floor(Math.random() * 5) + 1\n        };\n        this.objects.push(object);\n      }\n    },\n\n    draw() {\n      this.ctx.clearRect(0, 0, this.width, this.height);\n      this.ctx.fillStyle = '#00FF00';\n      this.ctx.fillRect(this.basket.x, this.basket.y, this.basket.width, this.basket.height);\n\n      for (let i = 0; i < this.objects.length; i++) {\n        const object = this.objects[i];\n        this.ctx.fillStyle = object.color;\n        this.ctx.fillRect(object.x, object.y, object.width, object.height);\n        object.y += object.speed;\n\n        if (object.y + object.height > this.basket.y && object.x > this.basket.x && object.x < this.basket.x + this.basket.width) {\n          this.objects.splice(i, 1);\n          this.addObjects();\n          this.score += 1;\n        } else if (object.y + object.height > this.height) {\n          this.objects.splice(i, 1);\n          this.addObjects();\n        }\n      }\n\n      requestAnimationFrame(this.draw);\n    },\n\n    moveBasket(event) {\n      const speed = 10;\n\n      if (event.key === 'ArrowLeft') {\n        if (this.basket.x > 0) {\n          this.basket.x -= speed;\n        }\n      } else if (event.key === 'ArrowRight') {\n        if (this.basket.x + this.basket.width < this.width) {\n          this.basket.x += speed;\n        }\n      }\n    }\n\n  },\n\n  beforeUnmount() {\n    window.removeEventListener('keydown', this.moveBasket);\n  }\n\n};","map":{"version":3,"mappings":"AASA,eAAe;AACVA,OAAK,EAAE;AACFC,UAAM,EAAE;AACHC,UAAI,EAAEC,MADH;AAEHC,cAAQ,EAAE;AAFP;AADN,GADG;;AAOVC,MAAI,GAAG;AACF,WAAO;AACFC,iBAAW,EAAE;AADX,KAAP;AAGJ,GAXS;;AAYVC,SAAO,GAAG,CACT,CAbS;;AAcVC,SAAO,EAAE;AACJC,aAAS,GAAG;AACP,WAAKH,WAAL,GAAmB,IAAnB;AACA,WAAKI,UAAL;AACA,WAAKC,IAAL;AACAC,YAAM,CAACC,gBAAP,CAAwB,SAAxB,EAAmC,KAAKC,UAAxC;AACJ,KANG;;AAOJJ,cAAU,GAAG;AACR,YAAMK,UAAS,GAAI,CAAnB;;AACA,WAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAID,UAApB,EAAgCC,CAAC,EAAjC,EAAqC;AAChC,cAAMC,MAAK,GAAI;AACVC,WAAC,EAAEC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,KAAKC,KAAhC,CADO;AAEVC,WAAC,EAAE,KAAKtB,MAAL,GAAckB,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,MAAiB,KAAKpB,MAAL,GAAc,CAA/B,CAAX,CAFP;AAGVqB,eAAK,EAAE,EAHG;AAIVrB,gBAAM,EAAE,EAJE;AAKVuB,eAAK,EAAE,MAAML,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,QAA3B,EAAqCI,QAArC,CAA8C,EAA9C,CALH;AAMVC,eAAK,EAAEP,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,CAA3B,IAAgC;AAN7B,SAAf;AAQA,aAAKM,OAAL,CAAaC,IAAb,CAAkBX,MAAlB;AACL;AACJ,KApBG;;AAqBJN,QAAI,GAAG;AACF,WAAKkB,GAAL,CAASC,SAAT,CAAmB,CAAnB,EAAsB,CAAtB,EAAyB,KAAKR,KAA9B,EAAqC,KAAKrB,MAA1C;AACA,WAAK4B,GAAL,CAASE,SAAT,GAAqB,SAArB;AACA,WAAKF,GAAL,CAASG,QAAT,CAAkB,KAAKC,MAAL,CAAYf,CAA9B,EAAiC,KAAKe,MAAL,CAAYV,CAA7C,EAAgD,KAAKU,MAAL,CAAYX,KAA5D,EAAmE,KAAKW,MAAL,CAAYhC,MAA/E;;AACA,WAAK,IAAIe,IAAI,CAAb,EAAgBA,IAAI,KAAKW,OAAL,CAAaO,MAAjC,EAAyClB,CAAC,EAA1C,EAA8C;AACzC,cAAMC,MAAK,GAAI,KAAKU,OAAL,CAAaX,CAAb,CAAf;AACA,aAAKa,GAAL,CAASE,SAAT,GAAqBd,MAAM,CAACO,KAA5B;AACA,aAAKK,GAAL,CAASG,QAAT,CAAkBf,MAAM,CAACC,CAAzB,EAA4BD,MAAM,CAACM,CAAnC,EAAsCN,MAAM,CAACK,KAA7C,EAAoDL,MAAM,CAAChB,MAA3D;AACAgB,cAAM,CAACM,CAAP,IAAYN,MAAM,CAACS,KAAnB;;AACA,YAAIT,MAAM,CAACM,CAAP,GAAWN,MAAM,CAAChB,MAAlB,GAA2B,KAAKgC,MAAL,CAAYV,CAAvC,IAA4CN,MAAM,CAACC,CAAP,GAAW,KAAKe,MAAL,CAAYf,CAAnE,IAAwED,M