{"ast":null,"code":"export default {\n  data() {\n    return {\n      directionSequence: [],\n      gameStarted: false,\n      message: '',\n      level: 1\n    };\n  },\n\n  methods: {\n    startGame() {\n      this.gameStarted = true;\n      this.generateSequence();\n      window.addEventListener('keydown', this.checkInput);\n    },\n\n    generateSequence() {\n      const directions = ['Left', 'Up', 'Down', 'Right'];\n\n      for (let i = 0; i < this.level * 2 + 1; i++) {\n        this.directionSequence.push(directions[Math.floor(Math.random() * directions.length)]);\n      }\n    },\n\n    checkInput(event) {\n      const direction = event.key.replace('Arrow', '');\n      const index = this.directionSequence.indexOf(direction);\n\n      if (index !== -1) {\n        this.directionSequence.splice(index, 1);\n      }\n\n      if (this.directionSequence.length === 0) {\n        if (this.level === 3) {\n          this.message = '성공!';\n          window.removeEventListener('keydown', this.checkInput);\n        } else {\n          this.level++;\n          this.generateSequence();\n        }\n      }\n    }\n\n  },\n\n  beforeUnmount() {\n    window.removeEventListener('keydown', this.checkInput);\n  }\n\n};","map":{"version":3,"mappings":"AAiBC,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,uBAAiB,EAAE,EADd;AAELC,iBAAW,EAAE,KAFR;AAGLC,aAAO,EAAE,EAHJ;AAILC,WAAK,EAAE;AAJF,KAAP;AAMD,GARY;;AASbC,SAAO,EAAE;AACPC,aAAS,GAAG;AACV,WAAKJ,WAAL,GAAmB,IAAnB;AACA,WAAKK,gBAAL;AACAC,YAAM,CAACC,gBAAP,CAAwB,SAAxB,EAAmC,KAAKC,UAAxC;AACD,KALM;;AAMPH,oBAAgB,GAAG;AACjB,YAAMI,UAAS,GAAI,CAAC,MAAD,EAAS,IAAT,EAAe,MAAf,EAAuB,OAAvB,CAAnB;;AACA,WAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAI,KAAKR,KAAL,GAAa,CAAb,GAAiB,CAArC,EAAwCQ,CAAC,EAAzC,EAA6C;AAC3C,aAAKX,iBAAL,CAAuBY,IAAvB,CAA4BF,UAAU,CAACG,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgBL,UAAU,CAACM,MAAtC,CAAD,CAAtC;AACF;AACD,KAXM;;AAYPP,cAAU,CAACQ,KAAD,EAAQ;AAChB,YAAMC,SAAQ,GAAID,KAAK,CAACE,GAAN,CAAUC,OAAV,CAAkB,OAAlB,EAA2B,EAA3B,CAAlB;AACA,YAAMC,KAAI,GAAI,KAAKrB,iBAAL,