{"ast":null,"code":"export default {\n  data() {\n    return {\n      directionSequence: [],\n      userInput: '',\n      gameStarted: false,\n      message: '',\n      score: 0\n    };\n  },\n\n  methods: {\n    startGame() {\n      this.gameStarted = true;\n      this.generateSequence();\n    },\n\n    generateSequence() {\n      const directions = ['Left', 'Up', 'Down', 'Right'];\n\n      for (let i = 0; i < 10; i++) {\n        this.directionSequence.push(directions[Math.floor(Math.random() * directions.length)]);\n      }\n    },\n\n    checkInput() {\n      if (this.userInput === this.directionSequence.join('')) {\n        this.score++;\n        this.message = 'Correct!';\n      } else {\n        this.message = 'Incorrect. Try again.';\n      }\n\n      this.userInput = '';\n      this.directionSequence = [];\n      this.generateSequence();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaC,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,uBAAiB,EAAE,EADd;AAELC,eAAS,EAAE,EAFN;AAGLC,iBAAW,EAAE,KAHR;AAILC,aAAO,EAAE,EAJJ;AAKLC,WAAK,EAAE;AALF,KAAP;AAOD,GATY;;AAUbC,SAAO,EAAE;AACPC,aAAS,GAAG;AACV,WAAKJ,WAAL,GAAmB,IAAnB;AACA,WAAKK,gBAAL;AACD,KAJM;;AAKPA,oBAAgB,GAAG;AACjB,YAAMC,UAAS,GAAI,CAAC,MAAD,EAAS,IAAT,EAAe,MAAf,EAAuB,OAAvB,CAAnB;;AACA,WAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAI,EAApB,EAAwBA,CAAC,EAAzB,EAA6B;AAC3B,aAAKT,iBAAL,CAAuBU,IAAvB,CAA4BF,UAAU,CAACG,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgBL,UAAU,CAACM,MAAtC,CAAD,CAAtC;AACF;AACD,KAVM;;AAWPC,cAAU,GAAG;AACX,UAAI,KAAKd,SAAL,KAAmB,KAAKD,iBAAL,CAAuBgB,IAAvB,CAA4B,EAA5B,CAAvB,EAAwD;AACtD,aAAKZ,KAAL;AACA,aAAKD,OAAL,GAAe,UAAf;AACF,OAHA,MAGO;AACL,aAAKA,OAAL,GAAe,uBAAf;AACF;;AACA,WAAKF,SAAL,GAAiB,EAAjB;AACA,WAAKD,iBAAL,GAAyB,EAAzB;AACA,WAAKO,gBAAL;AACF;;AArBO;AAVI,CAAf","names":["data","directionSequence","userInput","gameStarted","message","score","methods","startGame","generateSequence","directions","i","push","Math","floor","random","length","checkInput","join"],"sourceRoot":"","sources":["C:\\Users\\kk\\Desktop\\Web\\Web-Project\