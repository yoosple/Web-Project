{"ast":null,"code":"export default {\n  mounted() {\n    this.canvas = this.$refs.canvas;\n    this.ctx = this.canvas.getContext('2d');\n    this.width = this.canvas.width;\n    this.height = this.canvas.height;\n    this.objects = [];\n    this.score = 0;\n    this.addObjects();\n    this.draw();\n  },\n\n  methods: {\n    addObjects() {\n      const numObjects = 5;\n\n      for (let i = 0; i < numObjects; i++) {\n        const object = {\n          x: Math.floor(Math.random() * this.width),\n          y: -Math.floor(Math.random() * this.height),\n          width: 50,\n          height: 50,\n          color: '#' + Math.floor(Math.random() * 16777215).toString(16),\n          speed: Math.floor(Math.random() * 5) + 1\n        };\n        this.objects.push(object);\n      }\n    },\n\n    draw() {\n      this.ctx.clearRect(0, 0, this.width, this.height);\n\n      for (let i = 0; i < this.objects.length; i++) {\n        const object = this.objects[i];\n        this.ctx.fillStyle = object.color;\n        this.ctx.fillRect(object.x, object.y, object.width, object.height);\n        object.y += object.speed;\n\n        if (object.y > this.height) {\n          this.objects.splice(i, 1);\n          this.addObjects();\n        }\n      }\n\n      this.score += 1;\n      requestAnimationFrame(this.draw);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAOG,eAAe;AACbA,SAAO,GAAG;AACR,SAAKC,MAAL,GAAc,KAAKC,KAAL,CAAWD,MAAzB;AACA,SAAKE,GAAL,GAAW,KAAKF,MAAL,CAAYG,UAAZ,CAAuB,IAAvB,CAAX;AACA,SAAKC,KAAL,GAAa,KAAKJ,MAAL,CAAYI,KAAzB;AACA,SAAKC,MAAL,GAAc,KAAKL,MAAL,CAAYK,MAA1B;AACA,SAAKC,OAAL,GAAe,EAAf;AACA,SAAKC,KAAL,GAAa,CAAb;AACA,SAAKC,UAAL;AACA,SAAKC,IAAL;AACD,GAVY;;AAWbC,SAAO,EAAE;AACPF,cAAU,GAAG;AACX,YAAMG,UAAS,GAAI,CAAnB;;AACA,WAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAID,UAApB,EAAgCC,CAAC,EAAjC,EAAqC;AACnC,cAAMC,MAAK,GAAI;AACbC,WAAC,EAAEC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,KAAKb,KAAhC,CADU;AAEbc,WAAC,EAAE,CAACH,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,KAAKZ,MAAhC,CAFS;AAGbD,eAAK